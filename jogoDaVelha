.data
	tabuleiro: .word 1,2,3,4,5,6,7,8,9#vetor com 9 posições para o tabuleiro
	jogador1: .word 1
	jogador2: .word 0
	
.text
	#inicializar tabuleiro
inicializa_tabuleiro:

	la $t0, tabuleiro #carregar o primeiro enderço do vetor
	
	lw $s0,0($t0)
	lw $s1,4($t0)
	lw $s2,8($t0)
	lw $s3,12($t0)
	lw $s4,16($t0)
	lw $s5,20($t0)
	lw $s6,24($t0)
	lw $s7,28($t0)
	lw $a0,32($t0)	
